# Math-Functions
a <- exp(1)				# exponential function
print(filename = "Test results math-functions.txt", append = FALSE, a)

b <- ln(1)				# logarithmic function with natural base
print(filename = "Test results math-functions.txt", append = TRUE, b)

c <- sqrt(16)			# square root function
print(filename = "Test results math-functions.txt", append = TRUE, c)

d <- power(2,2)			# power function: power(a,b) = a^b
print(filename = "Test results math-functions.txt", append = TRUE, d)

e <- abs(-3.2)			# absolute value function
print(filename = "Test results math-functions.txt", append = TRUE, e)

f <- sin(0.0)			# sin function
print(filename = "Test results math-functions.txt", append = TRUE, f)

g <- cos(0.0)			# cos function
print(filename = "Test results math-functions.txt", append = TRUE, g)

