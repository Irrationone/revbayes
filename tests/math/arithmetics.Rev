
# Simple mathematical operators
a <- 1 + 1				# Addition
print(filename = "Test results arithmetics.txt", append = FALSE, a)

b <- 10 - 5				# Subtraction
print(filename = "Test results arithmetics.txt", append = TRUE, b)

c <- 5 * 5				# Multiplication
print(filename = "Test results arithmetics.txt", append = TRUE, c)

d <- 10 / 2				# Division
print(filename = "Test results arithmetics.txt", append = TRUE, d)

e <- 2^3				# Exponentiation
print(filename = "Test results arithmetics.txt", append = TRUE, e)

