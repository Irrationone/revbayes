# loops
sum <- 0
for (i in 1:100) {
   sum <- sum + i
}
print(filename = "Test results for loops.txt", append = FALSE, "Sum of 1:100:		", sum)

# Fibonacci series
fib[1] <- 1
fib[2] <- 1
for (j in 3:10) {
   fib[j] <- fib[j-1] + fib[j-2]
}

print(filename = "Test results for loops.txt", append = TRUE, "Fibonacci series of 1:10:	", fib)

# factorial calculations as while loop
fac <- 1
i <- 10
while (i > 1) {
   fac <- fac * i
   i <- i - 1
}
print(filename = "Test results for loops.txt", append = TRUE, "Factorial 10:		", fac)







