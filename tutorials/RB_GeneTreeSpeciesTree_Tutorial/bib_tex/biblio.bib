issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Bacteria,Ecosystem,Genome: Bacterial,Marine Biology,Models: Biological,Molecular Sequence Data,Photobacterium,Sphingomonadaceae},
