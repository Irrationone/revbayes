issn = {1018-4813},
journal = {European journal of human genetics : EJHG},
keywords = {Chromosome Mapping,Chromosome Mapping: methods,Databases, Genetic,Genetic Predisposition to Disease,Genetic Vectors,Genome, Human,Genotype,Humans,Models, Genetic,Models, Statistical,Multigene Family,Phenotype},
month = may,
