pages = {241--4},
pmid = {22037308},
publisher = {Nature Publishing Group},
title = {{Ecology drives a global network of gene exchange connecting the human microbiome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22037308},
volume = {480},
year = {2011}
}
