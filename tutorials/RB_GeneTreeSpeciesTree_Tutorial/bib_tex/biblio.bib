journal = {Molecular Biology and Evolution},
keywords = {Bacteria,Characters,G+C Content,Genome,Inference,Maximum Likelihood,Mitochondrial,Molecular Phylogeny,Nonhomogeneous Model,Nucleotide Substitutions,Protein,Rates,Trees},
