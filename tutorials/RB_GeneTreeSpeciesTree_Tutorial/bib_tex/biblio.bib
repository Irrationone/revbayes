pages = {14144--14149},
title = {{Parallel inactivation of multiple GAL pathway genes and ecological diversification in yeasts}},
url = {papers2://publication/uuid/819403FB-8AD5-48C6-8008-3937153611DA http://www.pnas.org/content/101/39/14144.long papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p2512},
