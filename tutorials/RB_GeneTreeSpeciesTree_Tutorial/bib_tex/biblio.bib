pages = {1309--1313},
title = {{Genomic and morphological evidence converge to resolve the enigma of Strepsiptera.}},
url = {papers2://publication/uuid/C5D386D2-41AF-420E-B463-507ADEBCD410 http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=22704986\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1016/j.cub.2012.05.018},
