publisher = {Nature Publishing Group},
title = {{The highly reduced genome of an enslaved algal nucleus.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=11323671\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1038/35074092 papers2://publication/uuid/7A853EAE-9B09-47A1-BC9F-2508A1D2524B},
