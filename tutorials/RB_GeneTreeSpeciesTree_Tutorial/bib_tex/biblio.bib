file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Dauphas et al. - 2004 - Clues from Fe isotope variations on the origin of early Archean BIFs from Greenland.pdf:pdf},
issn = {1095-9203},
journal = {Science},
month = dec,
