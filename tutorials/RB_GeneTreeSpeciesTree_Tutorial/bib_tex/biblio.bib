keywords = {Bacteria,Gene Transfer: Horizontal,Models: Genetic,Phylogeny,Stochastic Processes},
month = aug,
number = {4},
pages = {633--642},
title = {{A model of horizontal gene transfer and the bacterial phylogeny problem}},
