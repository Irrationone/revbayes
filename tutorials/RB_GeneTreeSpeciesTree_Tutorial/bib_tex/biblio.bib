issn = {1545-7885},
journal = {PLoS Biology},
month = jan,
number = {1},
pages = {e1000281},
publisher = {Public Library of Science},
title = {{The compartmentalized bacteria of the planctomycetes-verrucomicrobia-chlamydiae superphylum have membrane coat-like proteins}},
