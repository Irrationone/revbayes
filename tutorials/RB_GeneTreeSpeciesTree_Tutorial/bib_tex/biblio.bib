file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Elkins et al. - 2008 - A korarchaeal genome reveals insights into the evolution of the Archaea.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
