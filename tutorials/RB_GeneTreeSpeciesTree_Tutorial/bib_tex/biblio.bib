author = {Kasahara, Masahiro and Naruse, Kiyoshi and Sasaki, Shin and Nakatani, Yoichiro and Qu, Wei and Ahsan, Budrul and Yamada, Tomoyuki and Nagayasu, Yukinobu and Doi, Koichiro and Kasai, Yasuhiro and Jindo, Tomoko and Kobayashi, Daisuke and Shimada, Atsuko and Toyoda, Atsushi and Kuroki, Yoko and Fujiyama, Asao and Sasaki, Takashi and Shimizu, Atsushi and Asakawa, Shuichi and Shimizu, Nobuyoshi and Hashimoto, Shin-Ichi and Yang, Jun and Lee, Yongjun and Matsushima, Kouji and Sugano, Sumio and Sakaizumi, Mitsuru and Narita, Takanori and Ohishi, Kazuko and Haga, Shinobu and Ohta, Fumiko and Nomoto, Hisayo and Nogata, Keiko and Morishita, Tomomi and Endo, Tomoko and Shin-I, Tadasu and Takeda, Hiroyuki and Morishita, Shinichi and Kohara, Yuji},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Kasahara et al. - 2007 - The medaka draft genome and insights into vertebrate genome evolution.pdf:pdf},
issn = {1476-4679},
journal = {Nature},
keywords = {Animals,China,Chromosomes,Evolution: Molecular,Fish Proteins,Genome,Genomics,Humans,Japan,Oryzias,Phylogeny,Polymorphism: Single Nucleotide,Sequence Homology: Nucleic Acid,Species Specificity,Taiwan,Time Factors},
