keywords = {Genetics, Population,Humans,Likelihood Functions,Markov Chains,Monte Carlo Method,Population Density,Transients and Migrants},
month = apr,
number = {8},
pages = {4563--8},
pmid = {11287657},
title = {{Maximum likelihood estimation of a migration matrix and effective population sizes in n subpopulations by using a coalescent approach.}},
