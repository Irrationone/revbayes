issn = {0168-9525},
journal = {Trends in genetics : TIG},
keywords = {006,0168-9525,09,10,1016,2013,additive model,all rights reserved,doi,dx,epistasis,fisher,genotype map,http,j,org,paradigm shift,phenotype,quantitative genetics,see front matter,tig,\ss 2013 elsevier ltd},
