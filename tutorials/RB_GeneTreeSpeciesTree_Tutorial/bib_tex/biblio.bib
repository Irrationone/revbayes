title = {{Simplifying gene trees for easier comprehension.}},
url = {papers2://publication/uuid/98DE28D6-3169-45C0-B8EF-FAD13E7E1602 http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=16643669\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1186/1471-2105-7-231},
