issn = {1367-4803},
journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Computational Biology,Databases: Genetic,Likelihood Functions,Mathematics,Models: Genetic,Phylogeny,Software},
month = dec,
number = {24},
pages = {4338--4347},
title = {{Improving the efficiency of SPR moves in phylogenetic tree search methods based on maximum likelihood}},
url = {http://bioinformatics.oxfordjournals.org/cgi/content/full/21/24/4338?view=long\&pmid=16234323 papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p2603 papers2://publication/uuid/D56BF2FD-EE0D-45E6-AC2E-63B1E38E7FDB},
