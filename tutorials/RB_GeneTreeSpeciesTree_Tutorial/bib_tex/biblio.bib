keywords = {Basal Metabolism,DNA: Mitochondrial,Evolution: Molecular,Longevity,Mitochondria,Mutation,Oxidative Stress},
month = jun,
number = {3},
pages = {413--416},
title = {{Mitochondrial whims: metabolic rate, longevity and the rate of molecular evolution}},
