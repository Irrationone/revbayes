keywords = {Amino Acid Sequence,Animals,Base Sequence,Conserved Sequence,Evolution: Molecular,Fishes,Insulin-Like Growth Factor II,Molecular Sequence Data,Phylogeny,Selection: Genetic,Sequence Alignment,Time Factors},
