journal = {Systematic biology},
keywords = {Animals,Base Sequence,Bombyx,Computer Simulation,DNA,Escherichia coli,Evolution: Molecular,Models: Genetic,Myoglobin,Nucleic Acid Conformation,Phylogeny,Protein Conformation,Proteins,RNA,Sequence Alignment,Sperm Whale},
