file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Nelson et al. - 2008 - The Population Reference Sample, POPRES a resource for population, disease, and pharmacological genetics research.pdf:pdf},
issn = {1537-6605},
