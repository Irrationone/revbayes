issn = {1527-8204},
journal = {Annual review of genomics and human genetics},
keywords = {Animals,Data Interpretation: Statistical,Databases: Genetic,Gene Transfer: Horizontal,Genome,Genomics,Humans,Phylogeny,Polymorphism: Genetic,Recombination: Genetic,Sequence Alignment,Software},
month = jan,
pages = {217--231},
title = {{Phylogenetic inference using whole genomes}},
