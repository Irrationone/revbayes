author = {Gao, F and Bailes, E and Robertson, D L and Chen, Y Q and Rodenburg, C M and Michael, S F and Cummins, L B and Arthur, L O and Peeters, M and Shaw, G M and Sharp, P M and Hahn, B H},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Gao et al. - 1999 - Origin of HIV-1 in the chimpanzee Pan troglodytes troglodytes.pdf:pdf},
issn = {1476-4679},
journal = {Nature},
keywords = {Acquired Immunodeficiency Syndrome,Africa: Central,Africa: Eastern,Animals,Ape Diseases,Cercopithecus aethiops,DNA: Mitochondrial,DNA: Viral,Disease Outbreaks,Disease Reservoirs,Evolution: Molecular,Female,Genome: Viral,HIV-1,Humans,Meat,Molecular Sequence Data,Pan troglodytes,Phylogeny,Polymerase Chain Reaction,Recombination: Genetic,Sequence Alignment,Sequence Analysis: DNA,Simian Acquired Immunodeficiency Syndrome,Simian immunodeficiency virus,Zoonoses},
