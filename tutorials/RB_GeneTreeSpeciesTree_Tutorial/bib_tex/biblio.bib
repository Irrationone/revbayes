@article{Floudas2012,
abstract = {Wood is a major pool of organic carbon that is highly resistant to decay, owing largely to the presence of lignin. The only organisms capable of substantial lignin decay are white rot fungi in the Agaricomycetes, which also contains non-lignin-degrading brown rot and ectomycorrhizal species. Comparative analyses of 31 fungal genomes (12 generated for this study) suggest that lignin-degrading peroxidases expanded in the lineage leading to the ancestor of the Agaricomycetes, which is reconstructed as a white rot species, and then contracted in parallel lineages leading to brown rot and mycorrhizal species. Molecular clock analyses suggest that the origin of lignin degradation might have coincided with the sharp decrease in the rate of organic carbon burial around the end of the Carboniferous period.},
author = {Floudas, Dimitrios and Binder, Manfred and Riley, Robert and Barry, Kerrie and Blanchette, Robert A and Henrissat, Bernard and Mart\'{\i}nez, Angel T and Otillar, Robert and Spatafora, Joseph W and Yadav, Jagjit S and Aerts, Andrea and Benoit, Isabelle and Boyd, Alex and Carlson, Alexis and Copeland, Alex and Coutinho, Pedro M and de Vries, Ronald P and Ferreira, Patricia and Findley, Keisha and Foster, Brian and Gaskell, Jill and Glotzer, Dylan and G\'{o}recki, Paweł and Heitman, Joseph and Hesse, Cedar and Hori, Chiaki and Igarashi, Kiyohiko and Jurgens, Joel A and Kallen, Nathan and Kersten, Phil and Kohler, Annegret and K\"{u}es, Ursula and Kumar, T K Arun and Kuo, Alan and LaButti, Kurt and Larrondo, Luis F and Lindquist, Erika and Ling, Albee and Lombard, Vincent and Lucas, Susan and Lundell, Taina and Martin, Rachael and McLaughlin, David J and Morgenstern, Ingo and Morin, Emanuelle and Murat, Claude and Nagy, Laszlo G and Nolan, Matt and Ohm, Robin A and Patyshakuliyeva, Aleksandrina and Rokas, Antonis and Ruiz-Due\~{n}as, Francisco J and Sabat, Grzegorz and Salamov, Asaf and Samejima, Masahiro and Schmutz, Jeremy and Slot, Jason C and {St John}, Franz and Stenlid, Jan and Sun, Hui and Sun, Sheng and Syed, Khajamohiddin and Tsang, Adrian and Wiebenga, Ad and Young, Darcy and Pisabarro, Antonio and Eastwood, Daniel C and Martin, Francis and Cullen, Dan and Grigoriev, Igor V and Hibbett, David S},
doi = {10.1126/science.1221748},
