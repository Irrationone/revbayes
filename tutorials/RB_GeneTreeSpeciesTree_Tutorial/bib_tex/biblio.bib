month = jan,
pages = {257},
pmid = {17038185},
title = {{Integration of curated databases to identify genotype-phenotype associations.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1630430\&tool=pmcentrez\&rendertype=abstract},
volume = {7},
