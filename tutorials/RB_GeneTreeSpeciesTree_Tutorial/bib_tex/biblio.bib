@article{Capella-Gutierrez2013,
abstract = {MOTIVATION: Multiple sequence alignments are generally reconstructed using a progressive approach that follows a guide-tree. During this process, gaps are introduced at a cost to maximize residue pairing, but it is unclear whether inferred gaps reflect actual past events of sequence insertions or deletions. It has been found that patterns of inferred gaps in alignments contain information towards the true phylogeny, but it is as yet unknown whether gaps are simply reflecting information that was already present in the guide-tree.

RESULTS: We here develop a framework to disentangle the phylogenetic signal carried by gaps from that which is already present in the guide-tree. Our results indicate that most gaps are incorrectly inserted in patterns that, nevertheless, follow the guide-tree. Thus, most gap patterns in current alignments are not informative per se. This affects different programs to various degrees, PRANK being the most sensitive to the guide-tree.

SUPPLEMENTARY INFORMATION: Supplementary data are available at Bioinformatics online.},
author = {Capella-Guti\'{e}rrez, Salvador and Gabald\'{o}n, Toni},
doi = {10.1093/bioinformatics/btt095},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Capella-Guti\'{e}rrez, Gabald\'{o}n - 2013 - Measuring guide-tree dependency of inferred gaps in progressive aligners.pdf:pdf},
issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
month = apr,
number = {8},
pages = {1011--7},
pmid = {23435067},
title = {{Measuring guide-tree dependency of inferred gaps in progressive aligners.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3624810\&tool=pmcentrez\&rendertype=abstract},
volume = {29},
year = {2013}
}
