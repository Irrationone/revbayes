keywords = {Bayes Theorem,Computer Simulation,Evolution, Molecular,Markov Chains,Models, Genetic,Monte Carlo Method,Mutation Rate,Phylogeny,Stochastic Processes},
month = mar,
number = {3},
pages = {939--55},
pmid = {22049064},
title = {{A dirichlet process prior for estimating lineage-specific substitution rates.}},
