file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Kurabayashi et al. - 2006 - Complete nucleotide sequence of the mitochondrial genome of a Malagasy poison frog Mantella madagascariensis.pdf:pdf},
journal = {Molecular Phylogenetics and Evolution},
number = {1},
pages = {223--236},
publisher = {Elsevier Inc.},
title = {{Complete nucleotide sequence of the mitochondrial genome of a Malagasy poison frog Mantella madagascariensis: evolutionary implications on mitochondrial genomes of higher anuran groups.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=16446106\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1016/j.ympev.2005.11.021 papers2://publication/uuid/1A19DD5A-1D78-479F-B95B-27E4020D08D2},
