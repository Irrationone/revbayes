pages = {1701--1722},
title = {{Conjugate Gibbs sampling for Bayesian phylogenetic models.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=17238840\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1089/cmb.2006.13.1701 papers2://publication/uuid/C7FD6DE4-8DDA-42C9-BCAE-449BD604F5AB},
