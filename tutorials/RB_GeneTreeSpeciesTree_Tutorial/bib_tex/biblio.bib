pages = {e40503},
publisher = {Public Library of Science},
title = {{Hunter-gatherer energetics and human obesity.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=22848382\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1371/journal.pone.0040503 papers2://publication/uuid/DEAF7159-E895-4C65-9A37-02332EF6DBAE},
