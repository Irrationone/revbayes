issn = {14712148},
journal = {BMC Evolutionary Biology},
keywords = {Evolution: Molecular,Models: Genetic,RNA: Ribosomal,algorithms,computational biology,computational biology methods,computer simulation,evolution,genetic,likelihood functions,models,molecular,phylogeny,ribosomal,ribosomal genetics,rna,software},
