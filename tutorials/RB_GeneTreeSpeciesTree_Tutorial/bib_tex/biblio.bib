issn = {1088-9051},
journal = {Genome Research},
keywords = {Algorithms,Computational Biology,Europe,Gene Frequency,Genetic Association Studies,Genetics: Population,Genotype,Humans,Inflammatory Bowel Diseases,Jews,Likelihood Functions,Models: Genetic,Polymorphism: Single Nucleotide,Software,Time Factors},
month = sep,
number = {9},
pages = {1655--1664},
publisher = {Cold Spring Harbor Lab},
