@article{Drummond2005a,
abstract = {Intragenic recombination rapidly creates protein sequence diversity compared with random mutation, but little is known about the relative effects of recombination and mutation on protein function. Here, we compare recombination of the distantly related beta-lactamases PSE-4 and TEM-1 to mutation of PSE-4. We show that, among beta-lactamase variants containing the same number of amino acid substitutions, variants created by recombination retain function with a significantly higher probability than those generated by random mutagenesis. We present a simple model that accurately captures the differing effects of mutation and recombination in real and simulated proteins with only four parameters: (i) the amino acid sequence distance between parents, (ii) the number of substitutions, (iii) the average probability that random substitutions will preserve function, and (iv) the average probability that substitutions generated by recombination will preserve function. Our results expose a fundamental functional enrichment in regions of protein sequence space accessible by recombination and provide a framework for evaluating whether the relative rates of mutation and recombination observed in nature reflect the underlying imbalance in their effects on protein function.},
address = {Program in Computation and Neural Systems, Division of Chemistry and Chemical Engineering, California Institute of Technology, Pasadena, CA 91125, USA.},
author = {Drummond, D Allan and Silberg, Jonathan J and Meyer, Michelle M and Wilke, Claus O and Arnold, Frances H},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Drummond et al. - 2005 - On the conservative nature of intragenic recombination.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Evolution: Molecular,Genes,Models: Genetic,Mutagenesis,Mutation,Probability,Recombination: Genetic,beta-Lactamases},
month = apr,
number = {15},
pages = {5380--5385},
title = {{On the conservative nature of intragenic recombination}},
