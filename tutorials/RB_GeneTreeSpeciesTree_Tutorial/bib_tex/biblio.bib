month = nov,
number = {2},
pages = {412--426},
publisher = {SMBE},
title = {{Bayesian estimation of concordance among gene trees.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=17095535\&retmode=ref\&cmd=prlinks file:///Users/boussau/Documents/Papers2/2007/An\'{e}/Molecular Biology and Evolution 2007 An\'{e}.pdf papers2://publication/doi/10.1093/molbev/msl170 papers2://publication/uuid/14C1BD63-9C8F-4AF7-B34A-2708B18EF7DF papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p876},
