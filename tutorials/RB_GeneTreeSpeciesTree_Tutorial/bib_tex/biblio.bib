issn = {0169-5347},
journal = {Trends in ecology \& evolution (Personal edition)},
keywords = {Alleles,Bayes Theorem,Biodiversity,Brassicaceae,Evolution,Likelihood Functions,Phylogeny,Polymorphism: Genetic},
