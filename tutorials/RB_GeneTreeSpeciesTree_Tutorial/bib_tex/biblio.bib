issn = {0016-6731},
journal = {Genetics},
keywords = {Animals,Cell Nucleus,Codon,Computer Simulation,Drosophila,Drosophila melanogaster,Genes: Insect,Likelihood Functions,Models: Genetic,Mutation,Protein Biosynthesis,Selection: Genetic},
month = jan,
number = {1},
pages = {295--305},
title = {{Substitution rates in Drosophila nuclear genes: implications for translational selection}},
