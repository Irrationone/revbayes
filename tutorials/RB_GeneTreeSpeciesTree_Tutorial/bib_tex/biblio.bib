issn = {1537-1719},
journal = {Molecular Biology and Evolution},
keywords = {animals,evolution,gene duplication,molecular,multigene family,phylogeny,vertebrates,vertebrates genetics},
month = dec,
number = {3},
pages = {580--586},
pmid = {14694077},
publisher = {SMBE},
title = {{Phylogenetic dating and characterization of gene duplications in vertebrates: the cartilaginous fish reference}},
