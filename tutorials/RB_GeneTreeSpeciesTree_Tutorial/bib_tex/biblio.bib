issn = {1066-5277},
journal = {Journal of computational biology : a journal of computational molecular cell biology},
keywords = {Algorithms,Animals,Computational Biology,Computer Graphics,Computer Simulation,Evolution: Molecular,Haplotypes,Humans,Models: Genetic,Recombination: Genetic},
month = mar,
number = {2},
pages = {147--169},
title = {{Constructing minimal ancestral recombination graphs}},
url = {papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p323 papers2://publication/uuid/91F0DAED-48EC-4E51-BA24-5F54E92EEB73},
volume = {12},
