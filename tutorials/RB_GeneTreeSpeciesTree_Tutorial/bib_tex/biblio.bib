issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Amoeba,Animals,Dictyostelium,Entamoeba,Evolution,Genes,Humans,Molecular Sequence Data,Phylogeny},
month = feb,
