keywords = {Adaptation: Physiological,Base Sequence,Carbohydrate Epimerases,Consensus Sequence,Evolution: Molecular,Galactokinase,Galactose,Gene Silencing,Genome: Fungal,Molecular Sequence Data,Phylogeny,Sequence Analysis: DNA,Yeasts},
month = sep,
