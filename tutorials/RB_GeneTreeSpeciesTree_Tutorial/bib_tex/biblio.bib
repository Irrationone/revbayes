issn = {1095-9203},
journal = {Science},
keywords = {Amino Acid Substitution,Animals,Bayes Theorem,Daphnia,Evolution: Molecular,Female,Genes: Mitochondrial,Likelihood Functions,Male,Mitochondrial Proteins,Mutation,Parthenogenesis,Phylogeny,Recombination: Genetic,Selection: Genetic,Sex},
month = feb,
