journal = {BMC Bioinformatics},
pages = {113},
publisher = {BioMed Central Ltd},
title = {{MUSCLE: a multiple sequence alignment method with reduced time and space complexity.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=15318951\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1186/1471-2105-5-113 papers2://publication/uuid/70E69954-EF22-4158-A74C-528079F4A279},
