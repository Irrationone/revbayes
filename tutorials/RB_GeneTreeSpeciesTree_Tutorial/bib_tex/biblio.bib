journal = {FEMS microbiology letters},
keywords = {Chromosome Mapping,Computational Biology,Genome: Bacterial,Pseudomonas syringae,Sequence Analysis: DNA,Software},
month = feb,
number = {1},
pages = {103--111},
title = {{De novo assembly of the Pseudomonas syringae pv. syringae B728a genome using Illumina/Solexa short sequence reads}},
