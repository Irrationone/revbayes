journal = {Molecular biology and evolution},
keywords = {biased gene conversion,d n,d s,discovering genes influenced by,effective population size,for studying evolutionary processes,house finch,natural selection is central,positive selection,species of interest together,the ability to generate,transcriptome,transcriptomes in almost any,with},
month = may,
number = {5},
pages = {1046--50},
pmid = {23429858},
title = {{Evidence from a house finch (Haemorhous mexicanus) spleen transcriptome for adaptive evolution and biased gene conversion in passerine birds.}},
