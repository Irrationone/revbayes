issn = {1476-4679},
journal = {Nature},
keywords = {Animals,Base Sequence,Color,Drosophila,Drosophila Proteins,Evolution,Genes: Insect,Male,Molecular Sequence Data,Phylogeny,Pigmentation,Regulatory Sequences: Nucleic Acid,Response Elements,Wing},
