keywords = {evolution of sex,haplodiploidy,parthenogenesis},
publisher = {Annual Reviews  4139 El Camino Way, P.O. Box 10139, Palo Alto, CA 94303-0139, USA  },
title = {{THE EVOLUTION OF ALTERNATIVE GENETIC SYSTEMS IN INSECTS}},
