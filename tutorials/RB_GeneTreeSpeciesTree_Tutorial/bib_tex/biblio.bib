issn = {0944-2006},
journal = {Zoology (Jena, Germany)},
keywords = {Animals,Egg Shell,Microscopy: Electron: Scanning,Phylogeny,Species Specificity,Turtles},
month = jan,
number = {2},
pages = {127--136},
title = {{Testing phylogenetic implications of eggshell characters in side-necked turtles (Testudines: Pleurodira)}},
