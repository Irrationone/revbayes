issn = {1537-1719},
journal = {Molecular biology and evolution},
keywords = {Bayes Theorem,Borrelia burgdorferi,Borrelia burgdorferi: classification,Borrelia burgdorferi: genetics,Evolution, Molecular,Genetics, Population,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Software},
month = may,
