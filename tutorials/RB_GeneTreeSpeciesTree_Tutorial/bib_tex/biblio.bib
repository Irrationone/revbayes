institution = {Department of Molecular Evolution, Evolutionary Biology Center, Uppsala University, S-752 36 Uppsala, Sweden.},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {alphaproteobacteria,alphaproteobacteria classification,alphaproteobacteria genetics,alphaproteobacteria metabolism,animals,bacterial,bacterial genetics,chromosomes,eukaryotic cells,eukaryotic cells metabolism,evolution,genes,genome,humans,mitochondria,mitochondria genetics,mitochondria physiology,molecular,phylogeny,plants,plants microbiology},
