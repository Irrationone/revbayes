@article{Westesson2012,
abstract = {The Multiple Sequence Alignment (MSA) is a computational abstraction that represents a partial summary either of indel history, or of structural similarity. Taking the former view (indel history), it is possible to use formal automata theory to generalize the phylogenetic likelihood framework for finite substitution models (Dayhoff's probability matrices and Felsenstein's pruning algorithm) to arbitrary-length sequences. In this paper, we report results of a simulation-based benchmark of several methods for reconstruction of indel history. The methods tested include a relatively new algorithm for statistical marginalization of MSAs that sums over a stochastically-sampled ensemble of the most probable evolutionary histories. For mammalian evolutionary parameters on several different trees, the single most likely history sampled by our algorithm appears less biased than histories reconstructed by other MSA methods. The algorithm can also be used for alignment-free inference, where the MSA is explicitly summed out of the analysis. As an illustration of our method, we discuss reconstruction of the evolutionary histories of human protein-coding genes.},
address = {University of California Berkeley and University of California San Francisco Graduate Program in Bioengineering, University of California, Berkeley, California, United States of America.},
author = {Westesson, O and Lunter, G and Paten, B and Holmes, I},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Westesson et al. - 2012 - Accurate reconstruction of insertion-deletion histories by statistical phylogenetics.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
keywords = {Algorithms,Computer Simulation,Evolution: Molecular,Humans,INDEL Mutation,Likelihood Functions,Markov Chains,Models: Genetic,Open Reading Frames,Phylogeny,Sequence Alignment},
month = jan,
number = {4},
pages = {e34572},
title = {{Accurate reconstruction of insertion-deletion histories by statistical phylogenetics}},
url = {papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p4166},
volume = {7},
year = {2012}
}
@article{Westesson2012a,
abstract = {SUMMARY:  We describe handalign, a software package for Bayesian reconstruction of phylogenetic history. The underlying model of sequence evolution describes indels and substitutions. Alignments, trees, and model parameters are all treated as jointly-dependent random variables and sampled via Metropolis-Hastings Markov chain Monte Carlo (MCMC), enabling systematic statistical parameter inference and hypothesis testing. handalign implements several different MCMC proposal kernels, allows sampling from arbitrary target distributions via Hastings ratios, and uses standard file formats for trees, alignments and models.Availability and Implementation: Installation and usage instructions are at http://biowiki.org/HandAlign CONTACT:  ihh@berkeley.edu.},
address = {Department of Bioengineering, University of California Berkeley, CA, USA.},
author = {Westesson, Oscar and Barquist, Lars and Holmes, Ian},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Westesson, Barquist, Holmes - 2012 - HandAlign Bayesian multiple sequence alignment, phylogeny, and ancestral reconstruction.pdf:pdf},
journal = {Bioinformatics (Oxford, England)},
publisher = {Oxford Univ Press},
title = {{HandAlign: Bayesian multiple sequence alignment, phylogeny, and ancestral reconstruction.}},
url = {papers2://publication/uuid/C1381274-0FB9-4AD1-B9A0-CBA6EF2EA187 http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=22285828\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1093/bioinformatics/bts058},
