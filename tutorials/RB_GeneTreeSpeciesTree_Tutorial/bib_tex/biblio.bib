file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Kaiser, Bachtrog - Unknown - Evolution of Sex Chromosomes in Insects.pdf:pdf},
journal = {http://dx.doi.org/10.1146/annurev-genet-102209-163600},
keywords = {Y degeneration,dosage compensation,meiotic sex c},
