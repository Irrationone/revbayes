@article{Pacheco2011,
abstract = {Mitochondrial genes and genomes are among the major sources of data for evolutionary studies in birds. This places mitogenomic studies in birds at the core of intense debates in avian evolutionary biology. Indeed, complete mitochondrial genomes are actively been used to unveil the phylogenetic relationships among major orders whereas single genes (e.g. COX1) are considered standard for species identification and defining species boundaries (DNA barcoding). In this investigation, we study the time of origin and evolutionary relationships among Neoaves orders using complete mitochondrial genomes. First, we were able to solve polytomies previously observed at the deep nodes of the Neoaves phylogeny by analyzing 80 mitochondrial genomes, including 17 new sequences reported in this investigation. As an example, we found evidence indicating that columbiforms and charadriforms are sister groups. Overall, our analyses indicate that by improving the taxonomic sampling, complete mitochondrial genomes can solve the evolutionary relationships among major bird groups. Second, we used our phylogenetic hypotheses to estimate the time of origin of major avian orders as a way to test if their diversification took place prior to the Cretaceous/Tertiary (K/T) boundary. Such timetrees were estimated using several molecular dating approaches and conservative calibration points. Whereas we found time estimates slightly younger than those reported by others, most of the major orders originated prior to the K/T boundary. Finally, we used our timetrees to estimate the rate of evolution of each mitochondrial gene. We found great variation on the mutation rates among mitochondrial genes and within different bird groups. COX1 was the gene with less variation among Neoaves orders and the one with the least amount of rate heterogeneity across lineages. Such findings support the choice of COX 1 among mitochondrial genes as target for developing DNA barcoding approaches in birds.},
address = {Center for Evolutionary Medicine \& Informatics, The Biodesign Institute, Arizona State University, Tempe, AZ.},
author = {Pacheco, M Andre\'{\i}na and Battistuzzi, Fabia U and Lentino, Miguel and Aguilar, Roberto and Kumar, Sudhir and Escalante, Ananias A},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Pacheco et al. - 2011 - Evolution of modern birds revealed by Mitogenomics timing the radiation and origin of major orders.pdf:pdf},
journal = {Molecular Biology and Evolution},
publisher = {SMBE},
title = {{Evolution of modern birds revealed by Mitogenomics: timing the radiation and origin of major orders.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=21242529\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1093/molbev/msr014 papers2://publication/uuid/C00AD5F6-C9E3-4F1C-BCD8-9F61A08FE1C6},
year = {2011}
}
