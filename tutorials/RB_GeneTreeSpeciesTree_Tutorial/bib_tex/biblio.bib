pages = {780--791},
title = {{Phylogenetic analysis of the complete genome sequence of Encephalitozoon cuniculi supports the fungal origin of microsporidia and reveals a high frequency of fast-evolving genes.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=15599510\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1007/s00239-004-2673-0 papers2://publication/uuid/DB6A20F5-BDFC-4086-90AA-8F3785BE9DF5},
