title = {{Model misspecification and probabilistic tests of topology: evidence from empirical data sets.}},
url = {papers2://publication/uuid/CE448D21-9375-418C-B692-F352FA2A93E7 http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=12079647\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1080/10635150290069922},
