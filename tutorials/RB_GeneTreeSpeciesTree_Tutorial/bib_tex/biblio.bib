author = {Roshan, Usman W and Moret, Bernard M and Warnow, Tandy and Williams, Tiffani L},
file = {:Users/boussau/Library/Application Support/Mendeley Desktop/Downloaded/Roshan et al. - 2004 - Rec-I-DCM3 a fast algorithmic technique for reconstructing large phylogenetic trees.pdf:pdf},
