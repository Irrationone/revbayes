issn = {0028-0836},
journal = {Nature},
keywords = {Animals,Birds,DNA: Mitochondrial,Evolution: Molecular,Fossils,Genome,Phylogeny,Polymerase Chain Reaction,Sequence Analysis: DNA},
month = feb,
number = {6821},
pages = {704--707},
publisher = {Nature Publishing Group},
title = {{Complete mitochondrial genome sequences of two extinct moas clarify ratite evolution}},
url = {papers2://publication/uuid/AB2B4A52-B1E5-4C22-BF24-B7BF256DF1A8 papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p100},
volume = {409},
