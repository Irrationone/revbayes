issn = {0962-1075},
journal = {Insect molecular biology},
month = feb,
pages = {49--58},
title = {{Comparative analysis of nuclear tRNA genes of Nasonia vitripennis and other arthropods, and relationships to codon usage bias}},
url = {papers2://publication/uuid/EAADE230-180D-4564-AFF0-1087B034BD45 http://www3.interscience.wiley.com/cgi-bin/error?ErrorCode=4090\&CRETRY=1\&SRETRY=0 papers://c574a988-041f-48f0-a387-1407660e65a3/Paper/p2239},
