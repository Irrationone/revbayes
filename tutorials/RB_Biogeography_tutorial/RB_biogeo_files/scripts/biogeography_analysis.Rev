source("examples_in_progress/biogeography_DEC_1rate.Rev")

# get the mrca for all pairs of taxa
names <- tree.names()
ntips <- tree.ntips()
for (i in 1:ntips)
{
	for (j in 1:ntips)
	{
		cc <- clade(names[i],names[j])		
		tmp[j]<-mrcaIndex(cc,tree)
	}
	mrca_mtx[i] <- tmp
}

# index for common ancestor of
[ names[1], names[3] ]

# look up
mrca_mtx[1][3]
